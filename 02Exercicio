algoritmo "semnome"

var
nome: caracter
idade: inteiro
formacao: caracter
salario: real

inicio
escreval ("Digite seu nome completo: ")
leia(nome)

escreval("Digite a sua formação: ")
leia(formacao)

escreval("Digite a sua idade: ")
leia(idade)

escreval("Digite a sua pretensão salarial: ")
leia(salario)

SE ((idade >= 30) E (salario <= 2500)) ENTAO
escreval("Parabéns seu cadastro foi aprovado, entraremos em contato!")

SENAO
escreval("Infelizmente não temos vagas para seu perfil no momento!")

FIMSE

fimalgoritmo